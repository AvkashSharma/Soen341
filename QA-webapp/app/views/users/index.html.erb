<h1 align= "center">Users</h1>
<div align="center">

	<% @users.each do |user| %>

	<ul class = "listing">
	  <div class="row">
		  <div class="well col-md-4 col-md-offset-4">
			<li class="question-title">
				<%=link_to user.username, user_path(user) %>
			</li>
					
			<li><small> 
			<%= pluralize(user.questions.count, "question") if user.questions %> </small></li>
		<li><small>
			<%=link_to 'Edit', edit_user_path(user) %>
		</small></li>
		
		</div>
	</div>
	</ul>
	<% end %>
</div>


	 